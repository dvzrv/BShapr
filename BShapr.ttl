          	lv2:scalePoint [ rdfs:label "Off"; rdf:value 0 ] ;
						lv2:scalePoint [ rdfs:label "Audio in"; rdf:value 1 ] ;
						lv2:scalePoint [ rdfs:label "Constant"; rdf:value 2 ] ;
