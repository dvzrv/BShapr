            lv2:index 6 ;
            lv2:symbol "base" ;
            lv2:name "Base" ;
            lv2:portProperty lv2:integer, lv2:enumeration ;
						lv2:scalePoint [ rdfs:label "Seconds"; rdf:value 0 ] ;
						lv2:scalePoint [ rdfs:label "Beats"; rdf:value 1 ] ;
						lv2:scalePoint [ rdfs:label "Bars"; rdf:value 2 ] ;
